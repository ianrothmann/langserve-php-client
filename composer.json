{
    "name": "ianrothmann/langserve-php-client",
    "description": "A PHP Client for your LangServe project that is able to call API endpoints such as invoke, batch and stream.",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "symfony/http-client": ">=5.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IanRothmann\\LangServePhpClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ian Rothmann",
            "email": "ian@xtrapolate.io"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.35"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
